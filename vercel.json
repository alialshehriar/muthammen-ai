{
  "buildCommand": "echo 'Using pre-built dist folder'",
  "outputDirectory": "dist",
  "installCommand": "echo 'No install needed'",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

